%h2 Sign up
= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
  = f.input :email
  = f.input :password
  = f.input :password_confirmation
  = f.input :freeagent_subdomain, :hint => 'xxxx.freeagentcentral.com', :input_html => {size: 15}
  = f.input :freeagent_username
  = f.input :freeagent_password
  = f.submit "Sign up"
= render :partial => "devise/shared/links"
